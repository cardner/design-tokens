{
  "source": [
    "properties/**/*.json"
  ],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "duo-tokens/scss/",
      "files": [
        {
          "destination": "_globals.scss",
          "format": "scss/variables"
        }
      ]
    },
    "css": {
        "transformGroup": "css",
        "buildPath": "duo-tokens/css/",
        "files": [
          {
            "format": "css/variables",
            "destination": "variables.css"
            
          }
        ]
    }
  }
}
