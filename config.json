{
  "source": ["properties/**/*.json"],
  "platforms": {
    "css": {
        "transformGroup": "css",
        "buildPath": "duo-tokens/css/",
        "files": [
          {
            "destination": "variables.css",
            "format": "css/variables"
          }
        ]
    },
    "scss": {
      "transformGroup": "scss",
      "buildPath": "duo-tokens/scss/",
      "files": [{
        "destination": "_variables.scss",
        "format": "scss/variables"
      }]
    }
  }
}